<h1>Hello, <%= @user.email %>.</h1>
<div class="row">
  <% if @user == current_user %>
    <div class="col-md-8">
      <div class="new-items">
        <%= render partial: 'items/form', locals: { item: @item } %>
      </div>
    </div>
  <% end %>
</div>
<div class="row">
  <div class="col-md-8">
    <%= render partial: 'items/item', locals: { item: @item } %>
  </div>
</div>
